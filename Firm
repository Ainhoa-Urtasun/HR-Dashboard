import graphviz
Firm = graphviz.Digraph(node_attr={'shape':'record','color':'lightblue'})
Firm.node('n0',r'{SUPPORT|{Management|HRM|{Job Analysis and Design|HR Planning|Hiring|Performance Evaluation|Training|Career Development|Compensation}|Finance}}')
def CORE(text):
  Firm.node('n1',text)
Firm.edge('n0','n1')
