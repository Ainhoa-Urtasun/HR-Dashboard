import graphviz
Value-Chain = graphviz.Digraph(node_attr={'shape':'record'})
Value-Chain.node('n0',r'{SUPPORT ACTIVITIES|{Management|HRM|Finance|Accounting|Marketing}}',color='white',style='filled',fillcolor='lightblue')
def w(text):
  Value-Chain.node('n1',text,color='black',style='filled',fillcolor='lightpink')
Value-Chain.edge('n0','n1')
